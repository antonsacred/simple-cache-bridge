{
  "name": "sacred/simple-cache-from-cache-item-pool",
  "scripts": {
    "tests": "./vendor/bin/phpunit tests",
    "phpstan": "./vendor/bin/phpstan analyze src tests -l max"
  },
  "require": {
    "php": "^8.0",
    "psr/cache": "^2.0|^3.0",
    "psr/simple-cache": "^2.0|^3.0"
  },
  "require-dev": {
    "symfony/cache": "^5.4|^6.0",
    "phpunit/phpunit": "^9.5",
    "phpstan/phpstan": "^1.9",
    "phpstan/extension-installer": "^1.2",
    "phpstan/phpstan-strict-rules": "^1.4",
    "phpstan/phpstan-phpunit": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "Sacred\\Cached\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sacred\\Cached\\Tests\\": "tests"
    }
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  },
  "authors": [
    {
      "name": "Anton",
      "email": "antonsacred@gmail.com"
    }
  ]
}
